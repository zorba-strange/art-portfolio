<h1>Hello Art Index</h1>
<div class="image_index_flex">
  <div class="row">
    <% @art.each do |art| %>
      <div class="col-xs-6 col-md-3">
        <div class="thumbnail">
          <%= link_to image_tag(art.file_path), art_comments_path(art) %>
        </div>
      </div>
    <% end %>
  </div>
</div>

